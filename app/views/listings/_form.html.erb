<%= form_for(@listing) do |form| %>
  <%= form.label :Title %>
  <%= form.text_field :title%>

  <%= form.label :Address %>
  <%= form.text_field :address%>

  <%= form.label :Price %>
  <%= form.text_field :price%><br>

  <%= form.label :tag %>
  <%= form.collection_check_boxes :tag_ids, Tag.all, :id, :name, :item_wrapper_class => 'block' %>

  <%= form.submit %>
<%end%>