
<p>
  <strong>Title:</strong>
  <%= @listing.title%>
</p>

<p>
  <strong>Address:</strong>
  <%= @listing.address %>
</p>

<p>
  <strong>Price</strong>
  <%= @listing.price %>
</p>

<p>
<strong>Tag:</strong>
<% @listing.tags.each do |tag|%>
	<%= tag.name + ',' %>
<% end %>	

<% if current_user.id == @listing.user_id %>
	<%= link_to 'Edit', edit_listing_path(@listing) %> 
<% end %>	
<%= link_to 'Back', listings_path %>
